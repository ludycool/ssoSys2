--
-- Name: v_menubuttons; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW v_menubuttons AS
 SELECT rms_menubuttons.manuid,
    rms_menubuttons.orderno,
    rms_buttons.id,
    rms_buttons.buttonname,
    rms_buttons.remarks,
    rms_buttons.createtime,
    rms_buttons.modifytime,
    rms_buttons.valuename,
    rms_menubuttons.id AS menubuttonsid,
    rms_buttons.functionname,
    rms_buttons.icon
   FROM (rms_menubuttons
     JOIN rms_buttons ON (((rms_menubuttons.buttonid)::text = (rms_buttons.id)::text)));


ALTER TABLE v_menubuttons OWNER TO postgres;

--
-- Name: v_rms_menus; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW v_rms_menus AS
 SELECT rms_menus.projectid,
    rms_menus.companyid,
    rms_menus.valuename,
    rms_menus.typeid,
    rms_menus.isshow,
    rms_menus.isenable,
    rms_menus.modifytime,
    rms_menus.createtime,
    rms_menus.remarks,
    rms_menus.micon,
    rms_menus.icon,
    rms_menus.url,
    rms_menus.manuname,
    rms_menus.orderno,
    rms_menus.parentmanuid,
    rms_menus.id,
    rms_project.name AS projectname,
    rms_menus.level
   FROM (rms_menus
     LEFT JOIN rms_project ON (((rms_menus.projectid)::text = (rms_project.id)::text)));


ALTER TABLE v_rms_menus OWNER TO postgres;

--
-- Name: v_rms_project; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW v_rms_project AS
 SELECT rms_project.id,
    rms_project.companyid,
    rms_project.keys,
    rms_project.name,
    rms_project.remarks,
    rms_project.createtime,
    rms_project.modifytime,
    rms_project.isvalid,
    rms_project.isdeleted,
    rms_company.name AS companyname
   FROM (rms_project
     LEFT JOIN rms_company ON (((rms_project.companyid)::text = (rms_company.id)::text)));


ALTER TABLE v_rms_project OWNER TO postgres;

--
-- Name: v_role; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW v_role AS
 SELECT rms_role.id,
    rms_role.rolename,
    rms_role.remarks,
    rms_role.createby,
    rms_role.createtime,
    rms_role.modifyby,
    rms_role.modifytime,
    rms_role.roletypes,
    sysitem.itemname AS roletypesname,
    rms_role.companyid,
    rms_role.projectid,
    rms_project.name AS projectname,
    rms_role.jurisdiction
   FROM ((rms_role
     LEFT JOIN sysitem ON (((rms_role.roletypes)::text = (sysitem.itemvalue)::text)))
     LEFT JOIN rms_project ON (((rms_role.projectid)::text = (rms_project.id)::text)))
  WHERE ((sysitem.itemtype)::text = 'RoleTypes'::text);


ALTER TABLE v_role OWNER TO postgres;

--
-- Name: v_rolemanubuttons; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW v_rolemanubuttons AS
 SELECT rms_rolemanubuttons.roleid,
    rms_menubuttons.manuid,
    rms_menubuttons.orderno,
    rms_buttons.remarks,
    rms_buttons.createtime,
    rms_buttons.modifytime,
    rms_buttons.valuename,
    rms_buttons.functionname,
    rms_buttons.buttonname,
    rms_buttons.id,
    rms_buttons.icon,
    rms_buttons.color
   FROM ((rms_rolemanubuttons
     JOIN rms_menubuttons ON (((rms_rolemanubuttons.menubuttonsid)::text = (rms_menubuttons.id)::text)))
     JOIN rms_buttons ON (((rms_menubuttons.buttonid)::text = (rms_buttons.id)::text)));


ALTER TABLE v_rolemanubuttons OWNER TO postgres;

--
-- Name: v_rolemanus; Type: VIEW; Schema: public; Owner: postgres
