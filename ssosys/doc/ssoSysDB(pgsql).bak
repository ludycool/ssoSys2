ALTER TABLE v_sl_user OWNER TO postgres;

--
-- Name: v_sys_cityarea; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW v_sys_cityarea AS
 SELECT sys_cityarea.cityareaid,
    sys_cityarea.parentid,
    sys_cityarea.tname,
    sys_cityarea.tcode,
    sys_cityarea.areatypes,
    sys_cityarea.icon,
    sys_cityarea.remarks,
    sys_cityarea.createtime,
    sys_cityarea.updatetime,
    sys_cityarea.isvalid,
    sys_cityarea.isdeleted,
    sysitem.itemname AS areatypesname,
    sys_cityarea.orderno
   FROM (sys_cityarea
     LEFT JOIN sysitem ON (((sys_cityarea.areatypes)::text = (sysitem.itemvalue)::text)))
  WHERE ((sysitem.itemtype)::text = 'areatypes'::text);


ALTER TABLE v_sys_cityarea OWNER TO postgres;

--
-- Name: v_userrole; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW v_userrole AS
 SELECT rms_user.id,
    rms_user.loginname,
    rms_user.truename,
    rms_user.password,
    rms_user.createtime,
    rms_user.modifytime,
    rms_user.departmentid,
    rms_userrole.roleid,
    v_role.rolename,
    rms_user.usertype,
    rms_user.phone,
    rms_department.name AS departmentname,
    v_role.jurisdiction,
    rms_user.companyid,
    rms_company.name AS companyname,
    v_role.projectname
   FROM ((((rms_user
     LEFT JOIN rms_company ON (((rms_user.companyid)::text = (rms_company.id)::text)))
     LEFT JOIN rms_department ON (((rms_user.departmentid)::text = (rms_department.id)::text)))
     LEFT JOIN rms_userrole ON (((rms_user.id)::text = (rms_userrole.userid)::text)))
     LEFT JOIN v_role ON (((rms_userrole.roleid)::text = (v_role.id)::text)));


ALTER TABLE v_userrole OWNER TO postgres;

